{
  "name": "sonj-review",
  "version": "0.6.0",
  "description": "Extensible JSON Viewer",
  "main": "src/index.ts",
  "exportVar": "SonjReview",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "build+test": "npm run build && npm run test",
    "release": "rollup --environment RELEASE -c",
    "watch": "rollup -c --watch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxwroc/sonj-review.git"
  },
  "keywords": [
    "json",
    "viewer",
    "json-viewer",
    "sonj",
    "sonj-review"
  ],
  "author": "Max Chodorowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxwroc/sonj-review/issues"
  },
  "homepage": "https://maxwroc.github.io/sonj-review/",
  "jest": {
    "preset": "./jest-preset.cjs",
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "verbose": false,
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "reporters": [
      "default"
    ],
    "transform": {
      "^test/.+\\.ts?$": [
        "ts-jest",
        {
          "isolatedModules": true
        }
      ]
    }
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/expect-puppeteer": "^5.0.6",
    "@types/jest": "^29.5.13",
    "@types/jest-environment-puppeteer": "^5.0.6",
    "@types/puppeteer": "^7.0.4",
    "jest": "^29.7.0",
    "jest-puppeteer": "^10.1.1",
    "puppeteer": "^23.3.1",
    "rollup": "^4.21.3",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.2.5",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2"
  }
}
