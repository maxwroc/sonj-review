{
  "name": "sonj-review",
  "version": "0.3.0",
  "description": "Extensible JSON Viewer",
  "main": "dist/sonj-review.js",
  "types": "src/sonj-review.d.ts",
  "exportVar": "SonjReview",
  "type": "commonjs",
  "scripts": {
    "build": "rollup -c",
    "build+test": "npm run build && npm run test",
    "release": "rollup --environment RELEASE -c",
    "watch": "rollup -c --watch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxwroc/sonj-review.git"
  },
  "keywords": [
    "json",
    "viewer",
    "json-viewer",
    "sonj",
    "sonj-review"
  ],
  "author": "Max Chodorowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxwroc/sonj-review/issues"
  },
  "homepage": "https://maxwroc.github.io/sonj-review/",
  "jest": {
    "preset": "./jest-preset.js",
    "testMatch": [
      "**/test/*.test.ts"
    ],
    "verbose": false,
    "setupFilesAfterEnv": ["<rootDir>/jest-setup.ts"],
    "reporters": [
      "default",
      "jest-screenshot/reporter"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/expect-puppeteer": "^4.4.7",
    "@types/jest": "^27.0.2",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/puppeteer": "^5.4.4",
    "jest": "^27.3.0",
    "jest-puppeteer": "^6.0.0",
    "jest-screenshot": "^0.3.5",
    "puppeteer": "^10.4.0",
    "rollup": "^2.29.0",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.0.7",
    "tslib": "^2.0.2",
    "typescript": "^4.0.3"
  }
}
